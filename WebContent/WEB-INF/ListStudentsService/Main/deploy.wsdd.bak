<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from listStudentsService WSDL service -->

  <service name="listStudents" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Main"/>
      <parameter name="wsdlServiceElement" value="listStudentsService"/>
      <parameter name="schemaQualified" value="http://Main"/>
      <parameter name="wsdlServicePort" value="listStudents"/>
      <parameter name="className" value="Main.ListStudentsSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="listStudents"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="listAllStudents" qname="operNS:listAllStudents" xmlns:operNS="http://Main" returnQName="retNS:listAllStudentsReturn" xmlns:retNS="http://Main" returnType="rtns:listStudents" xmlns:rtns="http://Main" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="listAllStudents"/>

      <typeMapping
        xmlns:ns="http://Main"
        qname="ns:listStudents"
        type="java:Main.ListStudents_Type"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>

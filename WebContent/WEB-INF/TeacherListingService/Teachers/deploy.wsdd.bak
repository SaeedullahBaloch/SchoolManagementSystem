<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TeacherListingService WSDL service -->

  <service name="TeacherListing" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Teachers"/>
      <parameter name="wsdlServiceElement" value="TeacherListingService"/>
      <parameter name="schemaQualified" value="http://Teachers"/>
      <parameter name="wsdlServicePort" value="TeacherListing"/>
      <parameter name="className" value="Teachers.TeacherListingSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TeacherListing"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="listAllTeachers" qname="operNS:listAllTeachers" xmlns:operNS="http://Teachers" returnQName="retNS:listAllTeachersReturn" xmlns:retNS="http://Teachers" returnType="rtns:ListTeachers" xmlns:rtns="http://Teachers" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="listAllTeachers"/>

      <typeMapping
        xmlns:ns="http://Teachers"
        qname="ns:ListTeachers"
        type="java:Teachers.ListTeachers"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
